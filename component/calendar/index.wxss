.b {
    display: flex;
}

.lr {
    flex-direction: row;
}

.tb {
    flex-direction: column;
}

.pc {
    justify-content: center;
}

.ac,.cc {
    align-items: center;
}

.cc {
    justify-content: center;
}

.wrap {
    flex-wrap: wrap;
}

.flex {
    flex-grow: 1;
}

.bg {
    background-image: linear-gradient(180deg,#faefe7,#ffcbd7);
    overflow: hidden;
}

.white-color {
    color: #fff;
}

.fs24 {
    font-size: 24rpx;
}

.fs28 {
    font-size: 28rpx;
}

.fs32 {
    font-size: 32rpx;
}

.fs36 {
    font-size: 36rpx;
}

.calendar {
    box-sizing: border-box;
    width: 100%;
}

.handle {
    height: 80rpx;
}

.next-handle,.prev-handle {
    padding: 20rpx;
}

.date-in-handle {
    height: 80rpx;
}

.weeks {
    height: 50rpx;
    line-height: 50rpx;
    opacity: .5;
}

.week {
    text-align: center;
}

.grid,.week {
    width: 14.286014285714286%;
}

.date-wrap {
    height: 80rpx;
    width: 100%;
}

.date,.date-wrap {
    left: 0;
    position: relative;
    top: 0;
}

.date {
    animation-duration: .5s;
    animation-iteration-count: 1;
    animation-name: choosed;
    animation-timing-function: linear;
    border-radius: 50%;
    font-size: 26rpx;
    font-weight: 200;
    height: 55rpx;
    line-height: 55rpx;
    text-align: center;
    transition: all .3s;
    width: 55rpx;
}

.date-area-mode {
    border-radius: 0;
    width: 100%;
}

.date-desc {
    -webkit-line-clamp: 1;
    font-size: 20rpx;
    height: 32rpx;
    left: 50%;
    line-height: 32rpx;
    overflow: hidden;
    position: absolute;
    text-align: center;
    text-overflow: ellipsis;
    transform: translateX(-50%);
    white-space: nowrap;
    width: 150%;
    word-break: break-all;
}

@-webkit-keyframes choosed {
    from {
        transform: scale(1);
    }

    50% {
        transform: scale(.9);
    }

    to {
        transform: scale(1);
    }
}

@keyframes choosed {
    from {
        transform: scale(1);
    }

    50% {
        transform: scale(.9);
    }

    to {
        transform: scale(1);
    }
}

.todo-circle {
    border-style: solid;
    border-width: 1rpx;
    box-sizing: border-box;
}

.todo-dot {
    border-radius: 50%;
    height: 10rpx;
    left: 50%;
    position: absolute;
    transform: translateX(-50%);
    width: 10rpx;
}

.todo-dot-top {
    top: 3rpx;
}

.todo-dot.todo-dot-top-always {
    top: -8rpx;
}

.todo-dot.todo-dot-bottom {
    bottom: 0;
}

.todo-dot.todo-dot-bottom-always {
    bottom: -12rpx;
}

.date-desc.date-desc-top {
    top: -6rpx;
}

.date-desc.date-desc-top-always {
    top: -20rpx;
}

.date-desc.date-desc-bottom {
    bottom: -14rpx;
}

.todo-circle .date-desc.date-desc-bottom {
    bottom: -30rpx;
}

.date-desc.date-desc-bottom-always {
    bottom: -28rpx;
}